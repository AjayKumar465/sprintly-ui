{
  "name": "sprintly-ui",
  "title": "Sprintly UI",
  "description": "A React-based UI kit for building Sprintly interfaces",
  "version": "3.0.0",
  "main": "./src/index.js",
  "homepage": "https://github.com/sprintly/sprintly-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sprintly/sprintly-ui.git"
  },
  "bugs": {
    "url": "https://github.com/sprintly/sprintly-ui/issues"
  },
  "dependencies": {
    "array-find": "^1.0.0",
    "fuzzy": "^0.1.0",
    "lodash.pluck": "^3.1.2",
    "lodash.sortby": "^4.0.0",
    "lodash.union": "^4.0.1",
    "object-assign": "^4.0.1",
    "object-path": "^0.9.0",
    "react-click-outside": "^2.1.0"
  },
  "peerDependencies": {
    "moment": "^2.10.6",
    "react": "^0.14.6"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^11.1.0",
    "browserify-istanbul": "^0.2.1",
    "chai": "^3.2.0",
    "connect": "^3.4.0",
    "coveralls": "^2.11.2",
    "exorcist": "^0.4.0",
    "gulp": "^3.9.0",
    "gulp-mocha-phantomjs": "^0.10.1",
    "istanbul": "^0.3.20",
    "less": "^2.6.0",
    "mocha": "^2.4.5",
    "moment": "^2.10.6",
    "open": "^0.0.5",
    "phantomjs": "1.9.7-15",
    "react": "^0.14.6",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.6",
    "serve-static": "^1.10.0",
    "sinon": "^1.12.1",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ],
    "ignore": "sinon"
  },
  "istanbulify": {
    "ignore": [
      "**/node_modules/**",
      "**/test/**"
    ]
  },
  "scripts": {
    "prepublish": "NODE_ENV=production npm run build-min && npm run build-min-less",
    "build": "browserify src/index.js -d | node_modules/.bin/exorcist dist/js/sprintly-ui.js.map > dist/js/sprintly-ui.js",
    "build-min": "browserify src/index.js | uglifyjs -cm > dist/js/sprintly-ui.min.js",
    "build-less": "lessc src/less/sprintly-ui.less dist/css/sprintly-ui.css",
    "build-min-less": "npm run build-less && cat dist/css/sprintly-ui.css | csso -o dist/css/sprintly-ui.min.css",
    "watch": "watchify src/index.js -o 'node_modules/.bin/exorcist dist/js/sprintly-ui.js.map > dist/js/sprintly-ui.js' -d -v",
    "watch-less": "nodemon -e less -x 'npm run build-less'",
    "test": "gulp test",
    "build-test": "browserify test/index.js -d | node_modules/.bin/exorcist test/build.js.map > test/build.js",
    "watch-test": "watchify test/index.js -o 'node_modules/.bin/exorcist test/build.js.map > test/build.js' -d -v",
    "coverage": "gulp test-coverage && istanbul report --root coverage lcov && mv coverage/ test/",
    "coveralls": "cat test/coverage/lcov.info | coveralls",
    "clean": "rm test/build.js && rm -rf test/coverage/"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "keywords": [
    "sprintly",
    "react",
    "ui kit",
    "ui-kit",
    "ui library",
    "components",
    "ui components",
    "component library"
  ],
  "author": "Sprintly Team (http://sprint.ly/)",
  "license": "BSD-2-Clause"
}
